{
  "compilerOptions": {
    "rootDir": "./" ,
    "outDir": "./dist",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
    // "noEmit": true,
    // "allowImportingTsExtensions": true,
  },
}
